  "path": "docs/stories",
  "input": "story_graph_increments.json",
  "synchronizer": "synchronizers.story_io.DrawIOSynchronizer",
  "renderer_command": "render-discovery",
  "output": "story-map-discovery-increment-{increment_names}.drawio",
